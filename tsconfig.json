      "~/*": [ "src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"]
    },
   // "jsx": "react-jsx",
   // "jsxImportSource": "react"
  },
  "include": [
    ".astro/types.d.ts",
